    "postcss-loader": "^6.1.1",
    "prettier": "^2.3.2",
    "style-loader": "^3.0.0",
    "typescript": "^4.3.5",
    "webpack": "^5.44.0",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.7.2",
